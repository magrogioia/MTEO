@prefix ns0: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ns1: <http://rs.tdwg.org/dwc/terms/> .
@prefix ns2: <https://w3id.org/mteo#> .

<https://example.org/tanks/tropical-seas>
  a <https://w3id.org/mteo#AquariumTank> ;
  ns0:location "Aquarium of Genoa"^^xsd:string ;
  ns0:name "Tropical Seas Aquarium Tank"^^xsd:string .

<https://example.org/organisms/clownfish>
  a <https://w3id.org/mteo#MarineOrganism> ;
  ns1:conservationStatus "Least Concern"^^xsd:string ;
  ns1:scientificName "Amphiprion ocellaris"^^xsd:string ;
  ns0:name "Clownfish"^^xsd:string ;
  ns2:functionalNote "A reef-associated species that contributes to ecosystem balance."^^xsd:string ;
  ns2:locatedInTank <https://example.org/tanks/tropical-seas> ;
  ns2:playsEcologicalRole ns2:PreyRole .

<https://example.org/organisms/zebra-shark>
  a ns2:MarineOrganism ;
  ns1:conservationStatus "Endangered"^^xsd:string ;
  ns1:scientificName "Stegostoma tigrinum"^^xsd:string ;
  ns0:name "Zebra Shark"^^xsd:string ;
  ns2:functionalNote "Top predator helping regulate population dynamics."^^xsd:string ;
  ns2:locatedInTank <https://example.org/tanks/tropical-seas> ;
  ns2:playsEcologicalRole ns2:PredatorRole .

<https://example.org/organisms/emperor-angelfish>
  a ns2:MarineOrganism ;
  ns1:conservationStatus "Least Concern"^^xsd:string ;
  ns1:scientificName "Pomacanthus imperator"^^xsd:string ;
  ns0:name "Emperor Angelfish"^^xsd:string ;
  ns2:functionalNote "Omnivorous species interacting with both corals and algae."^^xsd:string ;
  ns2:locatedInTank <https://example.org/tanks/tropical-seas> ;
  ns2:playsEcologicalRole ns2:PredatorRole .

<https://example.org/organisms/montipora-coral>
  a ns2:Coral, ns0:Taxon ;
  ns1:conservationStatus "Least Concern"^^xsd:string ;
  ns1:scientificName "Montipora capricornis"^^xsd:string ;
  ns0:name "Montipora Coral"^^xsd:string ;
  ns0:taxonRank "species" ;
  ns0:sameAs ex:Montipora ;
  ns2:functionalNote "Provides habitat and absorbs Ca/Mg for skeletal growth, releases O2 via zooxanthellae."^^xsd:string ;
  ns2:absorbsSubstance "calcium, magnesium"^^xsd:string ;
  ns2:locatedInTank <https://example.org/tanks/tropical-seas> .

<https://example.org/organisms/ulva-alga>
  a ns2:Alga, ns0:Taxon ;
  ns1:scientificName "Ulva lactuca"^^xsd:string ;
  ns0:name "Ulva Alga"^^xsd:string ;
  ns0:taxonRank "species" ;
  ns0:sameAs ex:Ulva ;
  ns2:absorbsSubstance "nitrates, phosphates"^^xsd:string ;
  ns2:functionalNote "It absorbs excess nutrients, stabilizes water chemistry and grows rapidly."^^xsd:string ;
  ns2:growthSpeed "fast"^^xsd:string ;
  ns2:locatedInTank <https://example.org/tanks/tropical-seas> .

